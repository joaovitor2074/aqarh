    <script type="text/javascript">
	
		var contrast = false;

        function maisFonte(){
        	var tam = parseInt($('body').css('font-size'));
        	tam = tam + 2;
            $('body, p, span,label').animate({'font-size' : tam + 'px'});
	    };

	    function menosFonte(){
	    	var tam = parseInt($('body').css('font-size'));
        	tam = tam - 2;
            $('body, p, span,label').animate({'font-size' : tam + 'px'});
	    };
		
		function contraste(){
			if(contrast){
				$("body").removeClass('contrast');
				contrast = false;
			}else{
				$("body").addClass('contrast');
				contrast = true;
			}
	    };

	</script>
	
